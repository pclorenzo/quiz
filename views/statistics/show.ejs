<table>
	<tr>
		<td class="just_izq">Número total de preguntas</td>
		<td class="just_der_pad_10px"><%= stats.preguntas %></td>
	</tr>
	<tr>
		<td class="just_izq">Número total de cometarios</td>
		<td class="just_der_pad_10px"><%= stats.comentarios %></td>
	</tr>
	<tr>
		<td class="just_izq">Promedio cometarios/pregunta</td>
		<td class="just_der_pad_10px"><%= (stats.comentarios/stats.preguntas).toFixed(2) %></td>
	</tr>
	<tr>
		<td class="just_izq">Preguntas con comentarios</td>
		<td class="just_der_pad_10px"><%= stats.conComentarios %></td>
	</tr>
	<tr>
		<td class="just_izq">Preguntas sin comentarios</td>
		<td class="just_der_pad_10px"><%= stats.sinComentarios %></td>
	</tr>
</table>